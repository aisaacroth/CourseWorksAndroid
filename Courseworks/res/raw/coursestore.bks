export CLASSPATH=bcprov-jdk15on-150.jar
CERTSTORE=res/raw/coursestore.bks
if [ -a $CERTSTORE]; then
    rm $CERTSTORE || exit 1
fi
keytool \
      -import \
      -v \
      -trustcacerts \
      -alias 0\
      -file <(openssl x509 -in courseworkscert.pem) \
      -keystore $CERTSTORE \
      -storetype BKS \
      -provider org.bouncycastle.jce.provider.BouncyCastleProvider \
      -providerpath /usr/share/java/bcprov.jar \
      -storepass ez24get